<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="com.renting.domain">
 	<class name="com.renting.domain.Collection" table="collect_table">
 		<id name="collect_id" column="collect_id">
 		<!-- 主键生成策略 手动生成-->
 			<generator class="assigned"></generator>
 		</id>
 		<property name="user_id" column="user_id" ></property>
 		<property name="house_id" column="house_id" ></property>
 		<property name="collect_time" column="collect_time" ></property>
 	</class>
 </hibernate-mapping>