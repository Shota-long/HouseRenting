<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="com.renting.domain">
 	<class name="com.renting.domain.BrowseRecord" table="browse_table">
 		<id name="browseId" column="browse_id">
 		<!-- 主键生成策略 手动生成-->
 			<generator class="assigned"></generator>
 		</id>
 		<property name="visitorId" column="visitor_id" ></property>
 		<property name="houseId" column="house_id" ></property>
 		<property name="browseTime" column="browse_time" ></property>
 			<property name="flag" column="flag" ></property>
 	</class>
 </hibernate-mapping>